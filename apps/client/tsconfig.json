{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@containers/*": ["src/containers/*"],
      "@assets/*": ["src/assets/*"],
      "@layouts/*": ["src/layouts/*"],
      "@stores/*": ["src/stores/*"],
      "@config/*": ["src/config/*"],
      "@server": ["../server/src/index"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
